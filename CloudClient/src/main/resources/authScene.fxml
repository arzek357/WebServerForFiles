<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.control.PasswordField?>
<AnchorPane fx:id="anchorPane" prefHeight="143.0" prefWidth="303.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="11.0" layoutY="50" prefHeight="71.0" prefWidth="168.0">
         <children>
            <Label fx:id="loginLabel" text="Login"/>
            <TextField fx:id="textField1"/>
            <Label fx:id="passLabel" text="Password"/>
            <PasswordField fx:id="textField2"/>
         </children>
      </VBox>
      <Button fx:id="enterButton" layoutX="200.0" layoutY="65.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="90.0" text="Вход" onAction="#button1press" />
      <Button fx:id="registrationButton" layoutX="200.0" layoutY="107.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="90.0" text="Регистрация" onAction="#button2press"/>
      <Label layoutX="11.0" layoutY="14.0" text="Авторизация в файловой системе">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
